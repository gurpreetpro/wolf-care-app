{
  "expo": {
    "name": "Pack",
    "slug": "pack-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pack.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Pride Connect to access your camera for video calls and photo verification.",
          "microphonePermission": "Allow Pride Connect to access your microphone for voice and video calls."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Pride Connect to access your photos to upload your profile picture."
        }
      ],
      "expo-screen-capture",
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow Pride Connect to access your camera for video calls.",
          "microphonePermission": "Allow Pride Connect to access your microphone for video calls."
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://pride-connect-server-6zn2uezcpq-el.a.run.app",
      "eas": {
        "projectId": "9d8a51f6-60ed-4e2c-86b9-556dacc7b452"
      }
    }
  }
}